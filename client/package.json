  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "react-hooks/exhaustive-deps": "off"
    }
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.css",
        "options": {
          "tabWidth": 4
        }
      }
    ],
    "tabWidth": 2,
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxSingleQuote": false,
    "singleQuote": true,
    "semi": true,
    "printWidth": 120
